{
  "name": "ngx-fluent-ui",
  "version": "0.0.0",
  "description": "Fluent UI icons library for angular",
  "author": "Benny Megidish",
  "license": "Apache-2.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennymeg/ngx-fluent-ui.git"
  },
  "keywords": [
    "angular",
    "microsoft",
    "fluentui",
    "fluent",
    "ui",
    "icons"
  ],
  "bugs": {
    "url": "https://github.com/bennymeg/ngx-fluent-ui/issues"
  },
  "homepage": "https://github.com/bennymeg/ngx-fluent-ui#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:lib": "ng serve fluent-ui-icons",
    "build:lib": "ng build fluent-ui-icons --configuration production",
    "test:lib": "ng test fluent-ui-icons",
    "lint:lib": "ng lint fluent-ui-icons",
    "serve:web": "ng serve fluent-ui-icons-web",
    "build:web": "ng build fluent-ui-icons-web --base-href=https://bennymeg.github.io/ngx-fluent-ui/ --configuration production",
    "deploy:web": "ng build fluent-ui-icons-web --base-href=https://bennymeg.github.io/ngx-fluent-ui/ --configuration production --outputPath=docs/",
    "publish": "cd dist/fluent-ui-icons && npm publish",
    "test:web": "ng test fluent-ui-icons-web",
    "lint:web": "ng lint fluent-ui-icons-web"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.3",
    "@angular/cdk": "^12.2.3",
    "@angular/common": "^12.2.3",
    "@angular/compiler": "^12.2.3",
    "@angular/core": "^12.2.3",
    "@angular/forms": "^12.2.3",
    "@angular/material": "^12.2.3",
    "@angular/platform-browser": "^12.2.3",
    "@angular/platform-browser-dynamic": "^12.2.3",
    "@angular/router": "^12.2.3",
    "ng-lazyload-image": "^9.1.0",
    "ngx-clipboard": "^14.0.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.1",
    "@angular-devkit/build-angular": "12.2.3",
    "@angular/cli": "^12.2.3",
    "@angular/compiler-cli": "^12.2.3",
    "@fluentui/svg-icons": "^1.1.137",
    "@types/jasmine": "^3.8.2",
    "@types/node": "^12.20.21",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.1",
    "protractor": "~7.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1"
  }
}
