{
  "name": "ngx-fluent-ui",
  "version": "0.0.0",
  "description": "Fluent UI icons library for angular",
  "author": "Benny Megidish",
  "license": "Apache-2.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennymeg/ngx-fluent-ui.git"
  },
  "keywords": [
    "angular",
    "microsoft",
    "fluentui",
    "fluent",
    "ui",
    "icons"
  ],
  "bugs": {
    "url": "https://github.com/bennymeg/ngx-fluent-ui/issues"
  },
  "homepage": "https://github.com/bennymeg/ngx-fluent-ui#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:lib": "ng serve fluent-ui-icons",
    "build:lib": "ng build fluent-ui-icons --configuration production",
    "test:lib": "ng test fluent-ui-icons",
    "lint:lib": "ng lint fluent-ui-icons",
    "serve:web": "ng serve fluent-ui-icons-web",
    "build:web": "ng build fluent-ui-icons-web --base-href=https://bennymeg.github.io/ngx-fluent-ui/ --configuration production",
    "deploy:web": "ng build fluent-ui-icons-web --base-href=https://bennymeg.github.io/ngx-fluent-ui/ --configuration production --outputPath=docs/",
    "publish": "cd dist/fluent-ui-icons && npm publish",
    "test:web": "ng test fluent-ui-icons-web",
    "lint:web": "ng lint fluent-ui-icons-web"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@fortawesome/angular-fontawesome": "^0.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "ng-lazyload-image": "^9.1.3",
    "ngx-clipboard": "^16.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular-devkit/build-angular": "14.2.13",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@fluentui/svg-icons": "^1.1.198",
    "@types/jasmine": "^3.8.2",
    "@types/node": "^12.20.21",
    "angular-cli-ghpages": "^2.0.3",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~5.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.1.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.6.4",
    "webpack": "^5.98.0"
  }
}